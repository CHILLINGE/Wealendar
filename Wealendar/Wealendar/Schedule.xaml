        mc:Ignorable="d"
        Title="Schedule" Height="400" Width="500">
    <Grid>
        <TextBox x:Name="sc" HorizontalAlignment="Left" Height="233" Margin="59,37,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="379"/>
